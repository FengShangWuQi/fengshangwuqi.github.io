{
  "name": "@fengshangwuqi/babel-preset",
  "version": "1.0.9",
  "author": "fengshangwuqi <fengshangwuqi@gmail.com>",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src/index.ts --out-file dist/index.cjs.js --presets=@babel/preset-env,@babel/preset-typescript",
    "prepare": "yarn clean && cross-env NODE_ENV=production yarn build"
  },
  "repository": "github:FengShangWuQi/fengshangwuqi.github.io",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.1",
    "@babel/plugin-proposal-optional-chaining": "7.10.3",
    "@babel/plugin-transform-runtime": "7.10.3",
    "@babel/plugin-transform-typescript": "7.10.3",
    "@babel/runtime-corejs3": "7.10.3"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-typescript": "7.10.1",
    "cross-env": "7.0.2",
    "rimraf": "3.0.2"
  },
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js"
}
