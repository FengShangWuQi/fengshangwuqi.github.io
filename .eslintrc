{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "double"],
    "no-console": 0,
    "arrow-parens": 0,

    "import/no-unresolved": 1,
    "import/no-extraneous-dependencies": 0,

    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
