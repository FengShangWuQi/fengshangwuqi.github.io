{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "scripts": {
    "clean": "rimraf public dist/${APP} .cache",
    "test": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "check": "npx npm-check -u -E",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "ts-node ./tools/index.ts",
    "development": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths && mkdir -p dist/${APP} && mv public/* dist/${APP}",
    "release": "gh-pages -d dist/${APP} -b ${BRANCH} -m deploy::${APP} && gh-pages-clean",
    "now-build": "gatsby build --prefix-paths"
  },
  "bin": {
    "tb": "./tools/taskbook/cjs/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.16",
    "@emotion/styled": "10.0.15",
    "@svgr/webpack": "4.3.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.9",
    "@types/typography": "0.16.2",
    "axios": "0.19.0",
    "core-js": "3.2.1",
    "emotion-theming": "10.0.14",
    "gatsby": "2.10.0",
    "gatsby-image": "2.2.1",
    "gatsby-plugin-emotion": "4.1.0",
    "gatsby-plugin-feed": "2.3.0",
    "gatsby-plugin-google-analytics": "2.1.0",
    "gatsby-plugin-offline": "2.2.0",
    "gatsby-plugin-page-creator": "2.1.0",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-plugin-sharp": "2.2.12",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.1.0",
    "gatsby-plugin-typescript": "2.1.0",
    "gatsby-plugin-typography": "2.3.0",
    "gatsby-remark-autolink-headers": "2.1.0",
    "gatsby-remark-copy-linked-files": "2.1.0",
    "gatsby-remark-emojis": "0.3.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.0",
    "gatsby-remark-prismjs": "3.3.0",
    "gatsby-remark-responsive-iframe": "2.2.0",
    "gatsby-remark-smartypants": "2.1.0",
    "gatsby-source-filesystem": "2.1.0",
    "gatsby-transformer-remark": "2.5.0",
    "gatsby-transformer-sharp": "2.2.6",
    "polished": "3.4.1",
    "prismjs": "1.17.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-spring": "8.0.27",
    "react-typography": "0.16.19",
    "rxjs": "6.5.2",
    "typescript": "3.6.2",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@emotion/babel-preset-css-prop": "10.0.14",
    "@types/cross-spawn": "6.0.0",
    "@types/fs-extra": "8.0.0",
    "@types/inquirer": "6.5.0",
    "@types/node": "12.7.3",
    "@types/prettier": "1.18.2",
    "babel-loader": "8.0.6",
    "chalk": "2.4.2",
    "cross-spawn": "6.0.5",
    "dotenv": "8.1.0",
    "fs-extra": "8.1.0",
    "gh-pages": "2.1.1",
    "globby": "10.0.1",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "ts-node": "8.3.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  }
}
