{
  "name": "fswq-blog",
  "private": true,
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://blog.fengshangwuqi.com/",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion",
    "pnpm"
  ],
  "appkit": {
    "dev": "gatsby develop -o",
    "build": "ts-node ./scripts/build/build-app.ts",
    "release": "ts-node scripts/release/publish.ts"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "pretest": "pnpm tsc",
    "test": "jest",
    "build::icons": "ts-node ./scripts/build/generate-icons.ts",
    "build::zhoubao": "ts-node ./scripts/build/generate-zhoubao.ts",
    "build::assets": "ts-node ./scripts/build/build-assets.ts",
    "r::create": "ts-node ./scripts/release/create-release.ts",
    "start": "ts-node scripts/start.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{png,jpg,jpeg,webp}": [
      "pnpm run build::assets"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@fengshangwuqi/eslint-config"
    ],
    "ignorePatterns": [
      "node_modules",
      "public",
      ".cache",
      "coverage"
    ],
    "rules": {
      "react/no-unescaped-entities": 0
    }
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "avoid",
    "bracketSameLine": true
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.1.0",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@sentry/gatsby": "6.13.2",
    "algoliasearch": "4.10.5",
    "facepaint": "1.2.1",
    "figlet": "1.5.2",
    "gatsby": "3.14.0",
    "gatsby-plugin-algolia": "0.22.2",
    "gatsby-plugin-catch-links": "3.14.0",
    "gatsby-plugin-emotion": "6.14.0",
    "gatsby-plugin-feed": "3.14.0",
    "gatsby-plugin-image": "1.9.0",
    "gatsby-plugin-mdx": "2.14.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-pnpm": "1.2.8",
    "gatsby-plugin-react-helmet-async": "1.2.0",
    "gatsby-plugin-react-svg": "3.0.1",
    "gatsby-plugin-sharp": "3.14.0",
    "gatsby-plugin-twitter": "3.14.0",
    "gatsby-plugin-typescript": "3.14.0",
    "gatsby-remark-autolink-headers": "4.11.0",
    "gatsby-remark-copy-linked-files": "4.11.0",
    "gatsby-remark-emojis": "0.4.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "5.11.0",
    "gatsby-remark-prismjs": "5.11.0",
    "gatsby-remark-responsive-iframe": "4.11.0",
    "gatsby-remark-smartypants": "4.11.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-sharp": "3.14.0",
    "immer": "9.0.6",
    "lmdb-store": "1.6.8",
    "lodash": "4.17.21",
    "polished": "4.1.3",
    "prismjs": "1.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.1.2",
    "react-instantsearch-dom": "6.12.1",
    "react-spring": "9.2.4",
    "rxjs": "7.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.15.4",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@fengshangwuqi/appkit": "1.1.50",
    "@fengshangwuqi/eslint-config": "1.1.42",
    "@fengshangwuqi/logger": "1.1.21",
    "@fengshangwuqi/ts-config": "1.1.7",
    "@squoosh/lib": "0.4.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.0",
    "@testing-library/react-hooks": "7.0.2",
    "@types/facepaint": "1.2.2",
    "@types/figlet": "1.5.4",
    "@types/fs-extra": "9.0.13",
    "@types/gh-pages": "3.2.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.174",
    "@types/node": "16.10.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-instantsearch-dom": "6.12.0",
    "@vercel/node": "1.12.1",
    "axios": "0.21.4",
    "babel-jest": "27.2.2",
    "babel-preset-gatsby": "1.14.0",
    "date-fns": "2.24.0",
    "dotenv": "10.0.0",
    "execa": "5.1.1",
    "fs-extra": "10.0.0",
    "gh-pages": "3.2.3",
    "globby": "11.0.4",
    "gray-matter": "4.0.3",
    "husky": "7.0.2",
    "jest": "27.2.2",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "react-test-renderer": "17.0.2",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "license": "MIT"
}
