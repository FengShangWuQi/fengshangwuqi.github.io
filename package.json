{
  "name": "fswq-blog",
  "private": true,
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://blog.fengshangwuqi.com/",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "appkit": {
    "dev": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths",
    "release": "gh-pages -d public -b {DEPLOY_BRANCH} && gh-pages-clean"
  },
  "scripts": {
    "clean": "gatsby clean",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "jest --coverage",
    "tsc": "tsc",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "appkit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test && yarn tsc",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@fengshangwuqi/eslint-config"
    ],
    "ignorePatterns": [
      "node_modules",
      "public",
      ".cache",
      "dist"
    ],
    "rules": {
      "@typescript-eslint/interface-name-prefix": 0,
      "@typescript-eslint/camelcase": 0
    }
  },
  "prettier": {
    "trailingComma": "all",
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "@mdx-js/mdx": "1.6.16",
    "@mdx-js/react": "1.6.16",
    "@svgr/webpack": "5.4.0",
    "axios": "0.20.0",
    "emotion-theming": "10.0.27",
    "facepaint": "1.2.1",
    "gatsby": "2.24.51",
    "gatsby-image": "2.4.16",
    "gatsby-plugin-catch-links": "2.3.11",
    "gatsby-plugin-emotion": "4.3.10",
    "gatsby-plugin-feed": "2.5.11",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-mdx": "1.2.36",
    "gatsby-plugin-offline": "3.2.25",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.29",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.3.10",
    "gatsby-plugin-typescript": "2.4.18",
    "gatsby-remark-autolink-headers": "2.3.11",
    "gatsby-remark-copy-linked-files": "2.3.12",
    "gatsby-remark-emojis": "0.4.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.3.27",
    "gatsby-remark-prismjs": "3.5.11",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-remark-responsive-iframe": "2.4.12",
    "gatsby-remark-smartypants": "2.3.10",
    "gatsby-source-filesystem": "2.3.26",
    "gatsby-transformer-sharp": "2.5.13",
    "immer": "7.0.8",
    "lodash": "4.17.20",
    "polished": "3.6.5",
    "prismjs": "1.21.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-spring": "8.0.27",
    "rxjs": "6.6.2"
  },
  "devDependencies": {
    "@fengshangwuqi/appkit": "1.0.14",
    "@fengshangwuqi/dev-deps": "1.0.16",
    "@fengshangwuqi/eslint-config": "1.0.23",
    "@fengshangwuqi/ts-config": "1.0.2",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "10.4.9",
    "@testing-library/react-hooks": "3.4.1",
    "@types/facepaint": "1.2.1",
    "@types/fs-extra": "9.0.1",
    "@types/globby": "9.1.0",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.12",
    "@types/lodash": "4.14.160",
    "@types/node": "14.6.0",
    "@types/react": "16.9.48",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "babel-jest": "26.3.0",
    "babel-preset-gatsby": "0.5.7",
    "chalk": "4.1.0",
    "fs-extra": "9.0.1",
    "gh-pages": "3.1.0",
    "globby": "11.0.1",
    "jest": "26.4.2",
    "react-test-renderer": "16.13.1"
  },
  "license": "MIT"
}
