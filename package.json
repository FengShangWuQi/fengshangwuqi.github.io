{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "react",
    "typescript",
    "glamor",
    "storybook",
    "gatsby"
  ],
  "scripts": {
    "clean": "npx rimraf public dist .cache",
    "test": "echo test",
    "format": "npx prettier --write '**/*.{js,ts,tsx,json,md}'",
    "start": "gatsby develop",
    "sbook": "start-storybook -p 9001 -c ./src-app/storybook",
    "mv::gatsby": "mkdir -p dist/blog && mv public/* dist/blog/",
    "build::gatsby": "gatsby build --prefix-paths && npm run mv::gatsby",
    "build::sbook": "build-storybook -c ./src-app/storybook -o dist/storybook",
    "build": "npm run clean && npm run build::gatsby && npm run build::sbook",
    "deploy": "npx gh-pages -d dist -r git@github.com:FengShangWuQi/fengshangwuqi.github.io.git -m deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test",
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "npx prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/lodash": "4.14.119",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "5.0.8",
    "gatsby": "2.0.79",
    "gatsby-plugin-feed": "2.0.11",
    "gatsby-plugin-google-analytics": "2.0.8",
    "gatsby-plugin-offline": "2.0.20",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sharp": "2.0.17",
    "gatsby-plugin-typescript": "2.0.3",
    "gatsby-remark-autolink-headers": "2.0.12",
    "gatsby-remark-copy-linked-files": "2.0.8",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.0.1",
    "gatsby-remark-prismjs": "3.2.0",
    "gatsby-remark-responsive-iframe": "2.0.8",
    "gatsby-remark-smartypants": "2.0.7",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-transformer-remark": "2.1.18",
    "gatsby-transformer-sharp": "2.1.10",
    "glamor": "2.20.40",
    "lodash": "4.17.11",
    "prismjs": "1.15.0",
    "react": "next",
    "react-dom": "next",
    "react-helmet": "5.2.0",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@storybook/addon-actions": "4.1.4",
    "@storybook/addon-console": "1.1.0",
    "@storybook/addon-knobs": "4.1.4",
    "@storybook/addon-links": "4.1.4",
    "@storybook/addon-options": "4.1.4",
    "@storybook/react": "4.1.4",
    "@types/storybook__react": "4.0.0",
    "awesome-typescript-loader": "5.2.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "npm-check": "5.9.0",
    "prettier": "1.15.3",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "rimraf": "2.6.2"
  },
  "license": "MIT"
}
