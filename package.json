{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "test": "jest --coverage",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "tsc": "tsc --noEmit",
    "pkg:build": "lerna exec -- rollup -c ../../rollup.config.js",
    "pkg:r": "lerna publish from-package --yes",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "ts-node ./tools/index.ts",
    "development": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths",
    "serve": "gatsby serve -o --prefix-paths",
    "repl": "gatsby repl",
    "release": "gh-pages -d public -b ${BRANCH} -m deploy::${APP} && gh-pages-clean",
    "now-build": "gatsby build --prefix-paths"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm-run-all tsc test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.23",
    "@mdx-js/mdx": "1.5.5",
    "@mdx-js/react": "1.5.5",
    "@svgr/webpack": "5.1.0",
    "axios": "0.19.1",
    "emotion-theming": "10.0.19",
    "facepaint": "1.2.1",
    "gatsby": "2.19.0",
    "gatsby-image": "2.2.39",
    "gatsby-plugin-catch-links": "2.1.24",
    "gatsby-plugin-emotion": "4.1.21",
    "gatsby-plugin-feed": "2.3.26",
    "gatsby-plugin-google-analytics": "2.1.33",
    "gatsby-plugin-mdx": "1.0.67",
    "gatsby-plugin-offline": "3.0.32",
    "gatsby-plugin-page-creator": "2.1.38",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.4.0",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.1.18",
    "gatsby-plugin-typescript": "2.1.26",
    "gatsby-remark-autolink-headers": "2.1.23",
    "gatsby-remark-copy-linked-files": "2.1.36",
    "gatsby-remark-emojis": "0.3.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.42",
    "gatsby-remark-prismjs": "3.3.30",
    "gatsby-remark-responsive-iframe": "2.2.31",
    "gatsby-remark-smartypants": "2.1.20",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-remark": "2.6.48",
    "gatsby-transformer-sharp": "2.3.13",
    "immer": "5.1.0",
    "lodash": "4.17.15",
    "polished": "3.4.2",
    "prismjs": "1.19.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-spring": "8.0.27",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "@testing-library/jest-dom": "5.0.0",
    "@testing-library/react": "9.4.0",
    "@testing-library/react-hooks": "3.2.1",
    "@types/cross-spawn": "6.0.1",
    "@types/facepaint": "1.2.1",
    "@types/fs-extra": "8.0.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.9.0",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.25",
    "@types/prettier": "1.19.0",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-jest": "24.9.0",
    "babel-preset-gatsby": "0.2.27",
    "chalk": "3.0.0",
    "cross-spawn": "7.0.1",
    "dotenv": "8.2.0",
    "eslint": "6.7.1",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "fs-extra": "8.1.0",
    "gh-pages": "2.2.0",
    "globby": "10.0.1",
    "husky": "4.0.10",
    "jest": "24.9.0",
    "lint-staged": "10.0.1",
