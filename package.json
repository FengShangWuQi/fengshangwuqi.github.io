{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "react",
    "typescript",
    "emotion",
    "polished",
    "storybook",
    "gatsby",
    "typography",
    "netlify"
  ],
  "scripts": {
    "test": "npm run lint",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint --fix gatsby-config.js gatsby-node.js src-app/sbook",
    "start": "ts-node ./tools/index.ts",
    "blog": "gatsby develop -o",
    "sbook": "start-storybook -p 9001 -c ./src-app/sbook",
    "clean::blog": "rimraf public dist/blog .cache",
    "build::blog": "npm run clean::blog && gatsby build --prefix-paths && mkdir -p dist/blog && mv public/* dist/blog",
    "build::sbook": "rimraf dist/sbook && build-storybook -c ./src-app/sbook -o dist/sbook",
    "r::blog": "npm run build::blog && gh-pages -d dist -m deploy blog",
    "r::sbook": "npm run build::sbook && gh-pages -d dist -m deploy sbook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.6",
    "@emotion/styled": "10.0.6",
    "@types/lodash": "4.14.119",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "5.0.8",
    "emotion-theming": "10.0.6",
    "gatsby": "2.0.91",
    "gatsby-plugin-emotion": "4.0.1",
    "gatsby-plugin-feed": "2.0.11",
    "gatsby-plugin-google-analytics": "2.0.9",
    "gatsby-plugin-offline": "2.0.21",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sharp": "2.0.17",
    "gatsby-plugin-typescript": "2.0.3",
    "gatsby-plugin-typography": "2.2.5",
    "gatsby-remark-autolink-headers": "2.0.12",
    "gatsby-remark-copy-linked-files": "2.0.8",
    "gatsby-remark-emojis": "0.2.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.0.1",
    "gatsby-remark-prismjs": "3.2.0",
    "gatsby-remark-responsive-iframe": "2.0.8",
    "gatsby-remark-smartypants": "2.0.7",
    "gatsby-source-filesystem": "2.0.16",
    "gatsby-transformer-remark": "2.2.0",
    "gatsby-transformer-sharp": "2.1.10",
    "lodash": "4.17.11",
    "polished": "2.3.3",
    "prismjs": "1.15.0",
    "react": "next",
    "react-dom": "next",
    "react-helmet": "5.2.0",
    "react-typography": "0.16.18",
    "typescript": "3.2.4",
    "typography": "0.16.18",
    "typography-theme-github": "0.16.18"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "@emotion/babel-preset-css-prop": "10.0.6",
    "@storybook/addon-options": "next",
    "@storybook/react": "next",
    "@types/cross-spawn": "6.0.0",
    "@types/inquirer": "0.0.43",
    "@types/node": "10.12.18",
    "@types/storybook__react": "4.0.0",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.0",
    "cross-spawn": "6.0.5",
    "dotenv": "6.2.0",
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "npm-check": "5.9.0",
    "prettier": "1.16.0",
    "rimraf": "2.6.3",
    "ts-node": "8.0.1"
  },
  "license": "MIT"
}
