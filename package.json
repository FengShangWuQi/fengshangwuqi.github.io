{
  "name": "fswq-blog",
  "private": true,
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "workspaces": [
    "packages/*"
  ],
  "preconstruct": {
    "packages": [
      "packages/*",
      "!packages/babel-preset"
    ]
  },
  "scripts": {
    "clean": "gatsby clean",
    "test": "tsc --noEmit && jest --coverage",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "pkg:build": "preconstruct build",
    "pkg:r": "lerna publish from-package --yes",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "ts-node ./tools/index.ts",
    "development": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths",
    "serve": "gatsby serve -o --prefix-paths",
    "repl": "gatsby repl",
    "release": "gh-pages -d public -b ${BRANCH} -m deploy::${APP} && gh-pages-clean",
    "now-build": "gatsby build --prefix-paths"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{json,md}": [
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@mdx-js/mdx": "1.5.7",
    "@mdx-js/react": "1.5.7",
    "@svgr/webpack": "5.1.0",
    "axios": "0.19.2",
    "emotion-theming": "10.0.27",
    "facepaint": "1.2.1",
    "gatsby": "2.19.19",
    "gatsby-image": "2.2.41",
    "gatsby-plugin-catch-links": "2.1.25",
    "gatsby-plugin-emotion": "4.1.22",
    "gatsby-plugin-feed": "2.3.27",
    "gatsby-plugin-google-analytics": "2.1.35",
    "gatsby-plugin-mdx": "1.0.73",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-page-creator": "2.1.40",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.1.19",
    "gatsby-plugin-typescript": "2.1.27",
    "gatsby-remark-autolink-headers": "2.1.24",
    "gatsby-remark-copy-linked-files": "2.1.37",
    "gatsby-remark-emojis": "0.4.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.44",
    "gatsby-remark-prismjs": "3.3.31",
    "gatsby-remark-responsive-iframe": "2.2.32",
    "gatsby-remark-smartypants": "2.1.21",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-remark": "2.6.53",
    "gatsby-transformer-sharp": "2.3.14",
    "immer": "5.3.6",
    "lodash": "4.17.15",
    "polished": "3.4.4",
    "prismjs": "1.19.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-spring": "8.0.27",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@preconstruct/cli": "1.1.7",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "@testing-library/react-hooks": "3.2.1",
    "@types/cross-spawn": "6.0.1",
    "@types/facepaint": "1.2.1",
    "@types/fs-extra": "8.1.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.1.3",
