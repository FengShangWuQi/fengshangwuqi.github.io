{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "枫上雾棋的日志",
    "枫上雾棋",
    "fengshangwuqi"
  ],
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "rimraf public .cache *.log .DS_Store",
    "start": "gatsby develop",
    "lint": "eslint --ext .js --ignore-pattern public .",
    "new-blog": "gatsby-new-blog start",
    "build": "yarn run clean && gatsby build --prefix-paths",
    "deploy": "yarn run clean && gatsby build --prefix-paths && gh-pages -d public -r git@github.com:FengShangWuQi/fswq-blog.git -b master -m deploy",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "test": "yarn lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commitmsg": "commitlint -x @commitlint/config-conventional -e $GIT_PARAMS"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "gatsby": "^1.9.158",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-feed": "^1.3.18",
    "gatsby-plugin-google-analytics": "^1.0.19",
    "gatsby-plugin-no-sourcemaps": "^1.0.3",
    "gatsby-plugin-offline": "^1.0.13",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-sharp": "^1.6.28",
    "gatsby-plugin-twitter": "^1.0.15",
    "gatsby-remark-autolink-headers": "^1.4.12",
    "gatsby-remark-copy-linked-files": "^1.5.27",
    "gatsby-remark-emoji": "^0.0.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^1.5.41",
    "gatsby-remark-katex": "^1.0.12",
    "gatsby-remark-prismjs": "^1.2.12",
    "gatsby-remark-responsive-iframe": "^1.4.16",
    "gatsby-remark-smartypants": "^1.4.11",
    "gatsby-source-filesystem": "^1.5.15",
    "gatsby-transformer-remark": "^1.7.29",
    "gatsby-transformer-sharp": "^1.6.17",
    "gatsby-transformer-yaml": "^1.5.14",
    "i18next": "^10.3.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "qrcode": "^1.2.0",
    "react-helmet": "^5.2.0",
    "react-i18next": "^7.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-eslint": "^8.2.1",
    "conventional-changelog-cli": "^1.3.21",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.0",
    "figlet": "^1.2.0",
    "gatsby-new-blog": "1.0.4",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "inquirer": "^5.0.1",
    "lint-staged": "^7.0.3",
    "pinyin": "^2.8.3",
    "prettier": "^1.10.2",
    "regexpu-core": "^4.1.3",
    "rimraf": "^2.6.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "license": "MIT"
}
