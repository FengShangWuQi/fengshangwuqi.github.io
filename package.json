{
  "name": "fswq-blog",
  "private": true,
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://blog.fengshangwuqi.com/",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "appkit": {
    "dev": "gatsby develop -o",
    "build": "gatsby build --prefix-paths",
    "release": "ts-node script/release.ts"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "pnpm tsc && jest --coverage",
    "generate": "ts-node ./script/generate.ts",
    "start": "appkit",
    "debug:blog": "appkit dev blog",
    "debug:storybook": "appkit dev storybook"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@fengshangwuqi/eslint-config"
    ],
    "ignorePatterns": [
      "node_modules",
      "public",
      ".cache",
      "coverage"
    ],
    "rules": {
      "@typescript-eslint/interface-name-prefix": 0,
      "@typescript-eslint/camelcase": 0
    }
  },
  "prettier": {
    "trailingComma": "all",
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.1.0",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@sentry/gatsby": "6.5.1",
    "facepaint": "1.2.1",
    "figlet": "1.5.0",
    "gatsby": "3.6.2",
    "gatsby-plugin-catch-links": "3.6.0",
    "gatsby-plugin-emotion": "6.6.0",
    "gatsby-plugin-feed": "3.6.0",
    "gatsby-plugin-google-analytics": "3.6.0",
    "gatsby-plugin-image": "1.6.0",
    "gatsby-plugin-mdx": "2.6.0",
    "gatsby-plugin-offline": "4.6.0",
    "gatsby-plugin-pnpm": "1.2.6",
    "gatsby-plugin-react-helmet-async": "1.2.0",
    "gatsby-plugin-react-svg": "3.0.1",
    "gatsby-plugin-sharp": "3.6.0",
    "gatsby-plugin-twitter": "3.6.0",
    "gatsby-plugin-typescript": "3.6.0",
    "gatsby-remark-autolink-headers": "4.3.0",
    "gatsby-remark-copy-linked-files": "4.3.0",
    "gatsby-remark-emojis": "0.4.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "5.3.0",
    "gatsby-remark-prismjs": "5.3.0",
    "gatsby-remark-responsive-iframe": "4.3.0",
    "gatsby-remark-smartypants": "4.3.0",
    "gatsby-source-filesystem": "3.6.0",
    "gatsby-transformer-sharp": "3.6.0",
    "immer": "9.0.2",
    "lodash": "4.17.21",
    "polished": "4.1.3",
    "prismjs": "1.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.0.9",
    "react-spring": "9.2.1",
    "rxjs": "7.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.14.4",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@fengshangwuqi/appkit": "1.1.46",
    "@fengshangwuqi/eslint-config": "1.1.37",
    "@fengshangwuqi/logger": "1.1.17",
    "@fengshangwuqi/ts-config": "1.1.6",
    "@testing-library/jest-dom": "5.13.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.0",
    "@types/facepaint": "1.2.1",
    "@types/figlet": "1.5.1",
    "@types/fs-extra": "9.0.11",
    "@types/gh-pages": "3.0.0",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/node": "15.12.1",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "babel-jest": "27.0.2",
    "babel-preset-gatsby": "1.6.0",
    "chalk": "4.1.1",
    "date-fns": "2.22.1",
    "fs-extra": "10.0.0",
    "gh-pages": "3.2.0",
    "globby": "11.0.3",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "pretty-quick": "3.1.0",
    "react-test-renderer": "17.0.2",
    "ts-node": "10.0.0",
    "tslib": "2.2.0",
    "typescript": "4.3.2"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "license": "MIT"
}
