    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "babel-jest": "25.1.0",
    "babel-preset-gatsby": "0.2.29",
    "chalk": "3.0.0",
    "cross-spawn": "7.0.1",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "fs-extra": "8.1.0",
    "gh-pages": "2.2.0",
    "globby": "11.0.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lerna": "3.20.2",
