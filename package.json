{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "test": "jest --coverage",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "tsc": "tsc --noEmit",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "check": "npm-check -u -E",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "ts-node ./tools/index.ts",
    "development": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths",
    "serve": "gatsby serve -o --prefix-paths",
    "repl": "gatsby repl",
    "release": "gh-pages -d public -b ${BRANCH} -m deploy::${APP} && gh-pages-clean",
    "now-build": "gatsby build --prefix-paths"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tsc && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.23",
    "@mdx-js/mdx": "1.5.1",
    "@mdx-js/react": "1.5.1",
    "@svgr/webpack": "4.3.3",
    "axios": "0.19.0",
    "emotion-theming": "10.0.19",
    "facepaint": "1.2.1",
    "gatsby": "2.18.7",
    "gatsby-image": "2.2.34",
    "gatsby-plugin-catch-links": "2.1.19",
    "gatsby-plugin-emotion": "4.1.16",
    "gatsby-plugin-feed": "2.3.23",
    "gatsby-plugin-google-analytics": "2.1.29",
    "gatsby-plugin-mdx": "1.0.59",
    "gatsby-plugin-offline": "3.0.27",
    "gatsby-plugin-page-creator": "2.1.33",
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-sharp": "2.3.5",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.1.15",
    "gatsby-plugin-typescript": "2.1.20",
    "gatsby-remark-autolink-headers": "2.1.19",
    "gatsby-remark-copy-linked-files": "2.1.31",
    "gatsby-remark-emojis": "0.3.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.35",
    "gatsby-remark-prismjs": "3.3.25",
    "gatsby-remark-responsive-iframe": "2.2.28",
    "gatsby-remark-smartypants": "2.1.17",
    "gatsby-source-filesystem": "2.1.40",
    "gatsby-transformer-remark": "2.6.39",
    "gatsby-transformer-sharp": "2.3.7",
    "immer": "5.0.0",
    "lodash": "4.17.15",
    "polished": "3.4.2",
    "prismjs": "1.17.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-spring": "8.0.27",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/react-hooks": "3.2.1",
    "@types/cross-spawn": "6.0.1",
    "@types/facepaint": "1.2.1",
    "@types/fs-extra": "8.0.1",
    "@types/inquirer": "6.5.0",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.14",
    "@types/prettier": "1.19.0",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.14",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "babel-jest": "24.9.0",
    "babel-preset-gatsby": "0.2.23",
    "chalk": "3.0.0",
    "cross-spawn": "7.0.1",
    "dotenv": "8.2.0",
    "eslint": "6.7.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "fs-extra": "8.1.0",
    "gh-pages": "2.1.1",
    "globby": "10.0.1",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "npm-check": "5.9.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  }
}
