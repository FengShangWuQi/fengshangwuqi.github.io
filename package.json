{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "react",
    "typescript",
    "emotion",
    "polished",
    "storybook",
    "gatsby",
    "typography",
    "netlify"
  ],
  "scripts": {
    "clean": "rimraf public dist/blog .cache",
    "test": "tsc --noEmit && npm run lint",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint --fix gatsby-config.js gatsby-node.js src-app/sbook",
    "start": "ts-node ./tools/index.ts",
    "blog": "gatsby develop -o",
    "sbook": "start-storybook -p 9001 -c ./src-app/sbook",
    "build::blog": "npm run clean && gatsby build --prefix-paths && mkdir -p dist/blog && mv public/* dist/blog",
    "build::sbook": "rimraf dist/sbook && build-storybook -c ./src-app/sbook -o dist/sbook",
    "r::blog": "gh-pages -d dist/blog -b master -m deploy::blog && gh-pages-clean",
    "r::sbook": "gh-pages -d dist/sbook -m deploy::sbook && gh-pages-clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "@types/react-helmet": "5.0.8",
    "@types/three": "0.93.22",
    "@types/typography": "0.16.2",
    "emotion-theming": "10.0.7",
    "gatsby": "2.1.4",
    "gatsby-image": "2.0.29",
    "gatsby-plugin-emotion": "4.0.3",
    "gatsby-plugin-feed": "2.0.13",
    "gatsby-plugin-google-analytics": "2.0.14",
    "gatsby-plugin-offline": "2.0.23",
    "gatsby-plugin-react-helmet": "3.0.6",
    "gatsby-plugin-sharp": "2.0.20",
    "gatsby-plugin-twitter": "2.0.9",
    "gatsby-plugin-typescript": "2.0.8",
    "gatsby-plugin-typography": "2.2.7",
    "gatsby-remark-autolink-headers": "2.0.14",
    "gatsby-remark-copy-linked-files": "2.0.9",
    "gatsby-remark-emojis": "0.3.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.0.4",
    "gatsby-remark-prismjs": "3.2.4",
    "gatsby-remark-responsive-iframe": "2.0.9",
    "gatsby-remark-smartypants": "2.0.8",
    "gatsby-source-filesystem": "2.0.20",
    "gatsby-transformer-remark": "2.2.5",
    "gatsby-transformer-sharp": "2.1.13",
    "polished": "3.0.3",
    "prismjs": "1.15.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-helmet": "5.2.0",
    "react-typography": "0.16.18",
    "rxjs": "6.4.0",
    "three": "0.101.1",
    "typescript": "3.3.3333",
    "typography": "0.16.18"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@emotion/babel-preset-css-prop": "10.0.7",
    "@storybook/addon-options": "next",
    "@storybook/react": "next",
    "@types/cross-spawn": "6.0.0",
    "@types/inquirer": "0.0.44",
    "@types/node": "11.10.4",
    "@types/storybook__react": "4.0.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.1",
    "cross-spawn": "6.0.5",
    "dotenv": "6.2.0",
    "eslint": "5.15.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "npm-check": "5.9.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "ts-node": "8.0.2"
  },
  "license": "MIT"
}
