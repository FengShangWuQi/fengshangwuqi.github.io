{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "scripts": {
    "clean": "rimraf public dist/${APP} .cache",
    "test": "tsc --noEmit && npm run lint",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint --fix gatsby-config.js gatsby-node.js",
    "start": "ts-node ./tools/index.ts",
    "development": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths && mkdir -p dist/${APP} && mv public/* dist/${APP}",
    "release": "gh-pages -d dist/${APP} -b ${APP_BRANCH} -m deploy::${APP} && gh-pages-clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.9",
    "@emotion/styled": "10.0.9",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/react-helmet": "5.0.8",
    "@types/three": "0.93.27",
    "@types/typography": "0.16.2",
    "emotion-theming": "10.0.9",
    "gatsby": "2.1.37",
    "gatsby-image": "2.0.34",
    "gatsby-plugin-emotion": "4.0.6",
    "gatsby-plugin-feed": "2.0.15",
    "gatsby-plugin-google-analytics": "2.0.17",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.10",
    "gatsby-plugin-sharp": "2.0.29",
    "gatsby-plugin-twitter": "2.0.13",
    "gatsby-plugin-typescript": "2.0.11",
    "gatsby-plugin-typography": "2.2.10",
    "gatsby-remark-autolink-headers": "2.0.16",
    "gatsby-remark-copy-linked-files": "2.0.11",
    "gatsby-remark-emojis": "0.3.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.0.10",
    "gatsby-remark-prismjs": "3.2.5",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-remark-smartypants": "2.0.9",
    "gatsby-source-filesystem": "2.0.27",
    "gatsby-transformer-remark": "2.3.4",
    "gatsby-transformer-sharp": "2.1.17",
    "polished": "3.0.3",
    "prismjs": "1.15.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-helmet": "5.2.0",
    "react-spring": "8.0.15",
    "react-typography": "0.16.19",
    "rxjs": "6.4.0",
    "three": "0.102.1",
    "typescript": "3.3.3333",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@emotion/babel-preset-css-prop": "10.0.9",
    "@types/cross-spawn": "6.0.0",
    "@types/inquirer": "0.0.44",
    "@types/node": "11.11.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "cross-spawn": "6.0.5",
    "dotenv": "7.0.0",
    "eslint": "5.15.2",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "npm-check": "5.9.0",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "ts-node": "8.0.3"
  },
  "license": "MIT"
}
