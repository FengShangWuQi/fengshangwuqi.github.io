{
  "name": "fswq-blog",
  "private": true,
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "workspaces": [
    "packages/*"
  ],
  "appkit": {
    "dev": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths",
    "release": "gh-pages -d public -b master"
  },
  "scripts": {
    "clean": "gatsby clean && rimraf typings",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": " jest --coverage",
    "tsc": "tsc",
    "ncu": "ncu -u && lerna exec -- ncu -u",
    "pkg:clean": "lerna exec rimraf dist",
    "pkg:build": "lerna run --scope @fengshangwuqi/pkgkit build && lerna exec pkgkit build",
    "pkg:r": "lerna version patch --no-commit-hooks --yes",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "ts-node packages/appkit/bin/appkit.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test && yarn tsc",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./packages/eslint-config"
    ],
    "ignorePatterns": [
      "node_modules",
      "public",
      ".cache",
      "dist"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@mdx-js/mdx": "1.5.8",
    "@mdx-js/react": "1.5.8",
    "@svgr/webpack": "5.3.0",
    "axios": "0.19.2",
    "emotion-theming": "10.0.27",
    "facepaint": "1.2.1",
    "gatsby": "2.20.9",
    "gatsby-image": "2.3.1",
    "gatsby-plugin-catch-links": "2.2.1",
    "gatsby-plugin-emotion": "4.2.1",
    "gatsby-plugin-feed": "2.4.1",
    "gatsby-plugin-google-analytics": "2.2.2",
    "gatsby-plugin-mdx": "1.1.4",
    "gatsby-plugin-netlify-cms": "4.2.2",
    "gatsby-plugin-offline": "3.1.2",
    "gatsby-plugin-page-creator": "2.2.1",
    "gatsby-plugin-react-helmet": "3.2.1",
    "gatsby-plugin-sharp": "2.5.3",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.2.1",
    "gatsby-plugin-typescript": "2.3.1",
    "gatsby-remark-autolink-headers": "2.2.1",
    "gatsby-remark-copy-linked-files": "2.2.1",
    "gatsby-remark-emojis": "0.4.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.2.1",
    "gatsby-remark-prismjs": "3.4.1",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-remark-responsive-iframe": "2.3.1",
    "gatsby-remark-smartypants": "2.2.1",
    "gatsby-source-filesystem": "2.2.2",
    "gatsby-transformer-remark": "2.7.1",
    "gatsby-transformer-sharp": "2.4.3",
    "immer": "6.0.2",
    "lodash": "4.17.15",
    "netlify-cms-app": "2.12.0",
    "polished": "3.5.1",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-spring": "8.0.27",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.2",
    "@testing-library/react-hooks": "3.2.1",
    "@types/cross-spawn": "6.0.1",
    "@types/facepaint": "1.2.1",
    "@types/fs-extra": "8.1.0",
    "@types/globby": "9.1.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.8",
    "@types/react": "16.9.29",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "babel-jest": "25.2.4",
    "babel-preset-gatsby": "0.3.1",
    "chalk": "3.0.0",
    "cross-spawn": "7.0.1",
    "fs-extra": "9.0.0",
    "gh-pages": "2.2.0",
    "globby": "11.0.0",
    "jest": "25.2.4",
    "lerna": "3.20.2",
    "react-test-renderer": "16.13.1"
  },
  "license": "MIT"
}
