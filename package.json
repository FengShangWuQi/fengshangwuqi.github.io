{
  "name": "fswq-blog",
  "private": true,
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://blog.fengshangwuqi.com/",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "appkit": {
    "dev": "gatsby develop -o",
    "build": "gatsby build --prefix-paths",
    "release": "gh-pages -d public -b {DEPLOY_BRANCH} && gh-pages-clean"
  },
  "scripts": {
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "tsc",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "appkit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@fengshangwuqi/eslint-config"
    ],
    "ignorePatterns": [
      "node_modules",
      "public",
      ".cache",
      "coverage"
    ],
    "rules": {
      "@typescript-eslint/interface-name-prefix": 0,
      "@typescript-eslint/camelcase": 0
    }
  },
  "prettier": {
    "trailingComma": "all",
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@svgr/webpack": "5.5.0",
    "facepaint": "1.2.1",
    "gatsby": "2.31.0",
    "gatsby-image": "2.9.0",
    "gatsby-plugin-catch-links": "2.8.0",
    "gatsby-plugin-emotion": "5.2.0",
    "gatsby-plugin-feed": "2.11.0",
    "gatsby-plugin-google-analytics": "2.9.0",
    "gatsby-plugin-mdx": "1.8.0",
    "gatsby-plugin-offline": "3.8.0",
    "gatsby-plugin-react-helmet": "3.8.0",
    "gatsby-plugin-sharp": "2.12.2",
    "gatsby-plugin-svgr": "2.1.0",
    "gatsby-plugin-twitter": "2.8.0",
    "gatsby-plugin-typescript": "2.10.0",
    "gatsby-remark-autolink-headers": "2.9.0",
    "gatsby-remark-copy-linked-files": "2.8.0",
    "gatsby-remark-emojis": "0.4.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.9.0",
    "gatsby-remark-prismjs": "3.11.0",
    "gatsby-remark-responsive-iframe": "2.9.0",
    "gatsby-remark-smartypants": "2.8.0",
    "gatsby-source-filesystem": "2.9.1",
    "gatsby-transformer-sharp": "2.10.1",
    "immer": "8.0.1",
    "lodash": "4.17.20",
    "polished": "4.0.5",
    "prismjs": "1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-spring": "8.0.27",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.12.1",
    "@fengshangwuqi/appkit": "1.0.31",
    "@fengshangwuqi/dev-deps": "1.0.23",
    "@fengshangwuqi/eslint-config": "1.0.30",
    "@fengshangwuqi/ts-config": "1.0.4",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "5.0.0",
    "@types/facepaint": "1.2.1",
    "@types/fs-extra": "9.0.6",
    "@types/globby": "9.1.0",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.167",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "0.10.0",
    "chalk": "4.1.0",
    "date-fns": "2.16.1",
    "fs-extra": "9.0.1",
    "gh-pages": "3.1.0",
    "globby": "11.0.2",
    "jest": "26.6.3",
    "react-test-renderer": "17.0.1"
  },
  "license": "MIT"
}
