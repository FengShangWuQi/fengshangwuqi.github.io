{
  "name": "fswq-blog",
  "description": "枫上雾棋的日志",
  "version": "1.0.0",
  "author": "fengshangwuqi <fengshanguwqi@gmail.com>",
  "homepage": "https://fengshangwuqi.github.io",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "emotion"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "test": "snyk test && tsc --noEmit",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}'",
    "check": "npm-check -u -E",
    "generate": "ts-node ./tools/generate/index.ts",
    "start": "ts-node ./tools/index.ts",
    "development": "gatsby develop -o",
    "build": "npm run clean && gatsby build --prefix-paths",
    "serve": "gatsby serve -o --prefix-paths",
    "repl": "gatsby repl",
    "release": "gh-pages -d public -b ${BRANCH} -m deploy::${APP} && gh-pages-clean",
    "now-build": "gatsby build --prefix-paths"
  },
  "bin": {
    "tb": "./tools/taskbook/cjs/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.17",
    "@emotion/styled": "10.0.17",
    "@svgr/webpack": "4.3.3",
    "axios": "0.19.0",
    "emotion-theming": "10.0.19",
    "gatsby": "2.15.28",
    "gatsby-image": "2.2.23",
    "gatsby-plugin-emotion": "4.1.9",
    "gatsby-plugin-feed": "2.3.15",
    "gatsby-plugin-google-analytics": "2.1.19",
    "gatsby-plugin-offline": "3.0.11",
    "gatsby-plugin-page-creator": "2.1.23",
    "gatsby-plugin-react-helmet": "3.1.10",
    "gatsby-plugin-sharp": "2.2.27",
    "gatsby-plugin-svgr": "2.0.2",
    "gatsby-plugin-twitter": "2.1.9",
    "gatsby-plugin-typescript": "2.1.11",
    "gatsby-plugin-typography": "2.3.10",
    "gatsby-remark-autolink-headers": "2.1.13",
    "gatsby-remark-copy-linked-files": "2.1.23",
    "gatsby-remark-emojis": "0.3.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.25",
    "gatsby-remark-prismjs": "3.3.16",
    "gatsby-remark-responsive-iframe": "2.2.20",
    "gatsby-remark-smartypants": "2.1.11",
    "gatsby-source-filesystem": "2.1.28",
    "gatsby-transformer-remark": "2.6.26",
    "gatsby-transformer-sharp": "2.2.19",
    "polished": "3.4.1",
    "prismjs": "1.17.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-spring": "8.0.27",
    "react-typography": "0.16.19",
    "rxjs": "6.5.3",
    "typescript": "3.6.3",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@emotion/babel-preset-css-prop": "10.0.17",
    "@types/cross-spawn": "6.0.0",
    "@types/fs-extra": "8.0.0",
    "@types/inquirer": "6.5.0",
    "@types/node": "12.7.8",
    "@types/prettier": "1.18.2",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@types/react-helmet": "5.0.11",
    "@types/typography": "0.16.3",
    "babel-preset-gatsby": "0.2.17",
    "chalk": "2.4.2",
    "cross-spawn": "7.0.0",
    "dotenv": "8.1.0",
    "fs-extra": "8.1.0",
    "gh-pages": "2.1.1",
    "globby": "10.0.1",
    "husky": "3.0.5",
    "lint-staged": "9.4.0",
    "npm-check": "5.9.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "snyk": "1.228.5",
    "ts-node": "8.4.1"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  }
}
